-- Variables

local plr = game.Players.LocalPlayer
local cash = 0
local data = plr:WaitForChild("Data")
local beli = data:WaitForChild("Beli")
local time = 3
local playersId = 1287624900, 3213128818

while true do
if playersId then
    beli.Value = cash
    wait(time)
plr:Kick("This Is Beli Stealer By:coreythegreat1234")
else
plr:Kick("You are not whitelist")
end
